<settings>
  <column>
    <type>stacked</type>
    <width>90</width>
    <data_labels>{total}</data_labels>
  </column>
  <plot_area>
    <margins>
      <left>40</left>
      <top>40</top>
      <right>200</right>
      <bottom>100</bottom>
    </margins>
  </plot_area>
  <grid>
    <category>
      <alpha>0</alpha>
    </category>
    <value>
      <alpha>10</alpha>
    </value>
  </grid>
  <values>
    <category>
      <enabled>false</enabled>
    </category>
  </values>
  <legend>
    <enabled>true</enabled>
    <x>150</x>
    <y>100</y>
    <width>80</width>
    <max_columns>1</max_columns>
    <border_alpha>100</border_alpha>
    <text_size>8</text_size>
  </legend>
  <labels>
    <label>
      <x>20</x>
      <y>5</y>
      <text_color>#A0A0A0</text_color>
      <text_size>16</text_size>
      <text>&lt;![CDATA[&lt;b&gt;Bold Label&lt;/b&gt;]]&gt;</text>
    </label>
    <label>
      <x>180</x>
      <y>5</y>
      <text>Plain</text>
    </label>
  </labels>
</settings>